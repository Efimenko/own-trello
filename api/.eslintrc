{
  "plugins": [
    "prettier"
  ],
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "globals": {
    "console": true,
    "require": true,
    "process": true,
    "module": true,
    "exports": true,
    "Buffer": true
  },
  "rules": {
    "no-var": "error",
    "prettier/prettier": "error"
  }
}
